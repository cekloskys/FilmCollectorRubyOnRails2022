<h1>Delete <%= @actor.first_name %> <%= @actor.last_name %></h1>
<form action="/actors/<%= @actor.id %>" method="post">
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<div class="form-group">
		<label>First Name:</label>
		<input type="text" name="actor[first_name]" value="<%= @actor.first_name %>" disabled class="form-control">
	</div>
	<div class="form-group">
		<label>Last Name:</label>
		<input type="text" name="actor[last_name]" value="<%= @actor.last_name %>"disabled class="form-control">
	</div>
	<input type="submit" value="Delete" class="btn btn-primary">
</form>